#!/usr/bin/env sh

prefix="$1"
env="$2"


export KUBECONFIG="$HOME/.kube/config-$prefix-$env"
export VAULT_ADDR="http://vault.k8s-product.vault.$env.internal"
export AWS_PROFILE="$env"

ssocred $env

cat << EOF
####################################################
### Remember to disable NextDNS and start Access ###
####################################################
EOF
